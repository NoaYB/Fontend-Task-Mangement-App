             <button type=\\\"submit\\\">Add Task</button>\\n            </form>\\n            <ul>\\n                {tasks.map((task) => (\\n                    <li key={task.id}>\\n                        {task.description} - {task.completed ? 'Complete' : 'Incomplete'}\\n                        <button onClick={() => handleUpdateTask(task.id)}>Update</button>\\n                        <button onClick={() => handleDeleteTask(task.id)}>Delete</button>\\n                    </li>\\n                ))}\\n            </ul>\\n        </div>\\n    );\\n};\\n\\nexport default TaskList;\\n\\n\",\"import React from 'react';\\nimport { BrowserRouter as Router, Route, Routes } from 'react-router-dom'; // ודא שהייבוא נכון\\nimport Register from './components/Register';\\nimport Login from './components/Login';\\nimport TaskList from './components/TaskList';\\n\\nconst App = () => {\\n    return (\\n        <Router>\\n            <Routes> {/* Here we add tasklist */}\\n                <Route path=\\\"/register\\\" element={<Register />} />\\n                <Route path=\\\"/login\\\" element={<Login />} />\\n                <Route path=\\\"/tasks\\\" element={<TaskList />} />\\n            </Routes>\\n        </Router>\\n    );\\n};\\n\\nexport default App;\\n\\n\",\"import React from 'react';\\nimport ReactDOM from 'react-dom/client'; // עדכון לייבוא createRoot\\nimport App from './App'; // ייבוא רכיב ה-App שלך\\n\\n// מציאת אלמנט ה-root בדף ה-HTML\\nconst rootElement = document.getElementById('root');\\n\\n// יצירת root עם createRoot\\nconst root = ReactDOM.createRoot(rootElement);\\n\\n// רינדור רכיב ה-App\\nroot.render(\\n    <React.StrictMode>\\n        <App />\\n    </React.StrictMode>\\n);\\n\",\"module.exports = __webpack_public_path__ + \\\"static/media/api.986a97f6.bin\\\";\"]}","code":"(window.webpackJsonp=window.webpackJsonp||[]).push([[0],{17:function(e,t,a){\"use strict\";a.r(t);var n=a(0),r=a.n(n),l=a(6),c=a.n(l),o=a(8),s=a(2),u=a(7);var i=()=>{const[e,t]=Objec